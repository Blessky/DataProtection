{
    "version": "1.0.0-beta8",
    "description": "ASP.NET 5 logic to protect and unprotect data, similar to DPAPI.",
    "repository": {
        "type": "git",
        "url": "git://github.com/aspnet/dataprotection"
    },
    "dependencies": {
        "Microsoft.AspNet.Cryptography.Internal": "1.0.0-beta8",
        "Microsoft.AspNet.DataProtection.Abstractions": "1.0.0-beta8",
        "Microsoft.AspNet.DataProtection.Sources": {
            "type": "build",
            "version": "1.0.0-beta8"
        },
        "Microsoft.Framework.DependencyInjection.Abstractions": "1.0.0-beta8",
        "Microsoft.Framework.Logging.Abstractions": "1.0.0-beta8",
        "Microsoft.Framework.NotNullAttribute.Sources": {
            "type": "build",
            "version": "1.0.0-beta8"
        },
        "Microsoft.Framework.OptionsModel": "1.0.0-beta8"
    },
    "frameworks": {
        "net451": {
            "frameworkAssemblies": {
                "System.IO": "",
                "System.Security": "",
                "System.Xml": "",
                "System.Xml.Linq": ""
            }
        },
        "dnx451": {
            "frameworkAssemblies": {
                "System.IO": "",
                "System.Security": "",
                "System.Xml": "",
                "System.Xml.Linq": ""
            }
        },
        "dnxcore50": {
            "dependencies": {
                "Microsoft.Win32.Registry": "4.0.0-beta-23409",
                "System.Linq": "4.0.1-beta-23409",
                "System.Reflection.Extensions": "4.0.1-beta-23409",
                "System.Reflection.TypeExtensions": "4.0.1-beta-23409",
                "System.Security.Cryptography.X509Certificates": "4.0.0-beta-23409",
                "System.Security.Claims": "4.0.1-beta-23409",
                "System.Security.Principal.Windows": "4.0.0-beta-23409",
                "System.Xml.XDocument": "4.0.11-beta-23409"
            }
        }
    },
    "compilationOptions": {
        "allowUnsafe": true,
        "warningsAsErrors": true
    }
}