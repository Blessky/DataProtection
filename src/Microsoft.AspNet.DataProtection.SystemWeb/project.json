    },
    "frameworks": {
        "net451": {
            "dependencies": {
                "Microsoft.AspNet.DataProtection": "1.0.0-beta7",
                "Microsoft.Framework.DependencyInjection": "1.0.0-beta7"
            },
            "frameworkAssemblies": {
                "System.Configuration": "4.0.0.0",
                "System.Security": "4.0.0.0",
                "System.Web": "4.0.0.0"
            }
        }
    },
    "compilationOptions": {
        "warningsAsErrors": true
    }
